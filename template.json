{
  "builders": [{
    "type": "amazon-chroot",
    "source_ami": "ami-d05e75b8",
    "ami_name": "phila.gov-maintenance {{timestamp}}"
  }],
  "provisioners": [
    {
      "type": "file",
      "source": "nginx.conf",
      "destination": "/tmp/nginx.conf"
    },
    {
      "type": "file",
      "source": "maintenance.html",
      "destination": "/tmp/maintenance.html"
    },
    {
      "type": "shell",
      "script": "bootstrap.sh"
    }
  ]
}
